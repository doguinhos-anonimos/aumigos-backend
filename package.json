{
  "name": "fastify-stater",
  "version": "0.0.0",
  "description": "Uma configuração simples e poderosa para criar APIs com **Fastify**, **TypeScript**, **CORS**, **Token Auth**, **Env Config**, **Swagger**, **ZodTypeProvider** e Prisma para validação de tipos. Ideal para você começar rápido e fácil com o desenvolvimento de APIs escaláveis e seguras.",
  "main": "index.js",
  "scripts": {
    "start": "bun src/server.ts",
    "dev": "bun --hot src/server.ts",
    "test": "bunx vitest run",
    "test:watch": "bunx vitest",
    "test:e2e": "bunx vitest run -c vitest.config.ts --dir tests/routes",
    "test:unit": "bunx vitest run -c vitest.config.ts --dir tests/unit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/bun": "1.2.19",
    "@types/node": "^22.7.6",
    "@types/supertest": "^6.0.2",
    "prisma": "6.4.0",
    "supertest": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.3",
    "vitest": "^2.1.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@fastify/cors": "11.0.1",
    "@fastify/cookie": "^10.0.0",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/swagger": "^9.3.0",
    "@fastify/type-provider-typebox": "^5.0.0",
    "@prisma/client": "6.4.0",
    "@scalar/fastify-api-reference": "^1.32.1",
    "@sinclair/typebox": "^0.34.11",
    "bcryptjs": "3.0.2",
    "dotenv": "17.2.0",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "os": "^0.1.2"
  }
}